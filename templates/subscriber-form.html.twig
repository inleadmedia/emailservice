<form{{ attributes }}>

  {{ form.form_build_id }}
  {{ form.form_token }}
  {{ form.form_id }}

  <div class="form-group row">
    {#{{ form.email_address['#title'] }}#}
    {{ form.email_address }}
    {#{{ breakpoint() }}#}
  </div>


  <h3>{{ 'Preferences'|t }}</h3>

  <div class="row">
    <div class="col-6">
      <div class="form-group">
        {{ form.types['#options'] }}
      </div>
    </div>

    <div class="col-6">
      <div class="form-group">
        {{ form.categories }}
      </div>
    </div>
  </div>

  <div class="row">
    <div class="col-12">
      {{ form.actions }}
    </div>
  </div>
</form>
